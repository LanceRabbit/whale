<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h3>年齡設定</h3>
      <%= form_for [:cashier, @age] do |f| %>
        <div class="form-group">
         
          <%= f.label :age_type, "名稱" %>
          <%= f.text_field :age_type, class: "form-control"%>
          <br>
          
          <%= f.submit "確定", class: "btn btn-primary" %>
          
        </div>
      <% end %>
   </div>
  </div>
</div>
