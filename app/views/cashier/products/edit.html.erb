<div class="container">

  <%= form_for [:cashier, @product] do |f| %>
    <div class="form-group">

      <%= f.label :zh_name, "名稱" %>
      <%= f.text_field :zh_name, class: "form-control"%>
      <br>

      <%= f.label :quantity, "數量" %>
      <%= f.text_field :quantity, class: "form-control"%>
      <br>

      <%= f.submit "新增紀錄",class: "btn btn-primary" %>

    </div>
  <% end %>

</div>